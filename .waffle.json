{
    "compilerType": "solcjs",
    "compilerVersion": "0.5.6",
    "outputType": "all",
    "compilerOptions": {
      "outputSelection": {
        "*": {
          "*": [
            "evm.bytecode.object",
            "evm.deployedBytecode.object",
            "abi",
            "evm.bytecode.sourceMap",
            "evm.deployedBytecode.sourceMap",
            "metadata"
          ],
          "": ["ast"]
        }
      },
      "evmVersion": "petersburg",
      "optimizer": {
        "enabled": true,
        "runs": 999999
      }
    }
  }